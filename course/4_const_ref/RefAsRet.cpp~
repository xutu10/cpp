#include<iostream>
using namespace std;
#include<stdio.h>

int main(int argc, char *argv[])
{
    double a = 3.14;
	const int& ref = a;
	printf("a:%lf, ref: %d\n",a,ref);     //3.14 3
	printf("a:%d, ref: %d\n",&a,&ref);    //112 116

	int i = 3;
	const int& refI = i;
	printf("a:%d, ref: %d",&i,&refI);    //120 120
	
    return 0;
}
