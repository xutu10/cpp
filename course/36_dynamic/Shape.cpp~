using namespace std;

void Circle::draw(){
	cout<<"circle draw"<<endl;
}

Circle::~Circle(){
	cout<<"circle desconstructor"<<endl;
}

void Square::draw(){
	cout<<"square draw"<endl;
}

Square::~Circle(){
	cout<<"squre desconstructor"<<endl;
}

void Rectangle::draw(){
	cout<<"rectangle draw"<<endl;
}
Rectangle::~Circle(){
	cout<<"rectangle desconstructor"<<endl;
}
